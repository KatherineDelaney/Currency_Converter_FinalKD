{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "index.js" },
    { "src": "/action", "dest": "index.js" },
    { "src": "/about", "dest": "index.js" },
    { "src": "/help", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
